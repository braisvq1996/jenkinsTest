node {
  for (i=0; i<2; i++){
    stage "Stage #" +i
    print 'Hello word!!'
    
    if(i==0)
    {
      git "<a href="https://github.com/braisvq1996/jenkinsTest/JenkinsFile.git">"
      echo 'Runnig on Stage #0'        
    }
    else
    {
      build 'Declaretive pipeline'
      echo 'Running on Stage #1'
    }
  }
}
